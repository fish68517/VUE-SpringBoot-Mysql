# ==================================
#         通用忽略规则
# ==================================
# 日志、临时、备份文件
*.log
*.tmp
*.bak
*.swp
*~

# 操作系统生成的文件
.DS_Store
Thumbs.db

# IDE 和编辑器配置文件
# 使用 ** 可以匹配任意层级的目录
**/.idea/
**/*.iml
**/*.ipr
**/*.iws
.vscode/

# ==================================
#       Android 项目忽略规则
# ==================================
# 编译输出
/Android/build/
/Android/app/build/
/Android/captures/

# 本地配置文件 (包含 SDK 路径等敏感信息)
/Android/local.properties

# Gradle 本地缓存
.gradle/

# 生成的 APK/AAB 文件
/Android/*.apk
/Android/*.ap_
/Android/*.aab

# Java/Kotlin 编译产物
/Android/*.dex
/Android/**/*.class

# ==================================
#     Spring Boot 项目忽略规则
# ==================================
# Maven/Gradle 编译输出目录
/SpringBoot/target/
/SpringBoot/build/

# 编译后的 class 和 jar/war 包
/SpringBoot/**/*.class
/SpringBoot/*.jar
/SpringBoot/*.war
/SpringBoot/*.nar
/SpringBoot/*.ear

# ==================================
#         Vue 项目忽略规则
# ==================================
# 依赖包
/VUE/node_modules/
/VUE/time-management-ui/node_modules/


# 编译输出目录
/VUE/dist/

# 本地环境变量文件 (可能包含敏感信息)
/VUE/.env.local
/VUE/.env.*.local

# 缓存和锁文件
/VUE/*.cache
/VUE/*.lock
/VUE/yarn.lock
/VUE/pnpm-lock.yaml

# 测试覆盖率报告
/VUE/coverage/